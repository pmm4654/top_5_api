<% if current_search.present? %>
  <table id="search_history_table">
    <thead>
      <th><%= t('api_searches.index.searched_on') %></th>
    </thead>
    <% current_search.versions.order('created_at desc').each do |version| %>
      <tr>
        <td>
          <%= l(version.created_at, format: :short) %>
        </td>
      </tr>  
    <% end %>
  </table>
<% end %>

<script>
$(document).ready(function() {
    $('#search_history_table').DataTable();
});
  
</script>